  "bugs": {
    "url": "https://github.com/likr/boilerplate-react/issues"
  },
  "homepage": "https://github.com/likr/boilerplate-react#readme",
  "dependencies": {},
  "devDependencies": {
    "firebase-tools": "^3.13.1"
  }
}
